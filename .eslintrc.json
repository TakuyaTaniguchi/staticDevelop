{
    "env": {
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true,
              "trailingComma": "es5"
            }
          ]
    }
}